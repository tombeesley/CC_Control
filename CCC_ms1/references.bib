@article{castro2016,
  title = {Attentional Shifts in Categorization Learning: {{Perseveration}} but Not Learned Irrelevance},
  shorttitle = {Attentional Shifts in Categorization Learning},
  author = {Castro, Leyre and Wasserman, Edward A.},
