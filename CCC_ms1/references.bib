@article{huff2011,
  title = {Revealing {{Context-Specific Conditioned Fear Memories}} with {{Full Immersion Virtual Reality}}},
  author = {Huff, Nicole C. and Hernandez, Jose Alba and Fecteau, Matthew E. and Zielinski, David J. and Brady, Rachael and LaBar, Kevin S.},
